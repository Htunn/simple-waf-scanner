  {
    "name": "Fortinet FortiWeb",
    "vendor": "Fortinet",
    "detection": {
      "headers": {
        "server": "FortiWeb"
      },
      "body_patterns": [
        "FortiWeb",
        "FORTIGATE"
      ],
      "status_codes": [
        403
      ],
      "cookies": [
        "FORTIWAFSID"
      ]
    }
  },
  {
    "name": "Microsoft HTTPAPI (No WAF)",
    "vendor": "Microsoft",
    "detection": {
      "headers": {
        "server": "Microsoft-HTTPAPI"
      },
      "body_patterns": [],
      "status_codes": [],
      "cookies": []
    }
  }
]