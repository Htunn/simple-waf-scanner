[
  {
    "name": "Cloudflare",
    "vendor": "Cloudflare",
    "detection": {
      "headers": {
        "server": "cloudflare",
        "cf-ray": ".*",
        "cf-cache-status": ".*",
        "cf-request-id": ".*"
      },
      "body_patterns": [
        "cloudflare",
        "cf-browser-verification",
        "Attention Required! | Cloudflare",
        "CF-RAY",
        "checking your browser",
        "ray id:",
        "cloudflare-nginx"
      ],
      "status_codes": [
        403,
        503,
        520,
        521,
        522,
        523,
        524
      ],
      "cookies": [
        "__cfduid",
        "__cf_bm",
        "cf_clearance"
      ]
    }
  },
  {
    "name": "Azure Front Door",
    "vendor": "Microsoft Azure",
    "detection": {
      "headers": {
        "x-azure-ref": ".*",
        "x-fd-healthprobe": ".*",
        "x-azure-requestid": ".*",
        "x-azure-fdid": ".*"
      },
      "body_patterns": [
        "Azure Front Door",
        "Microsoft-Azure-Application-Gateway",
        "The request was blocked by Azure Front Door",
        "Our services aren't available right now"
      ],
      "status_codes": [
        403,
        502,
        503
      ],
      "cookies": [
        "ARRAffinity",
        "ARRAffinitySameSite"
      ]
    }
  },
  {
    "name": "AWS WAF",
    "vendor": "Amazon Web Services",
    "detection": {
      "headers": {
        "x-amzn-requestid": ".*",
        "x-amz-cf-id": ".*"
      },
      "body_patterns": [
        "Request blocked",
        "AWS WAF"
      ],
      "status_codes": [
        403
      ],
      "cookies": []
    }
  },
  {
    "name": "Akamai",
    "vendor": "Akamai Technologies",
    "detection": {
      "headers": {
        "server": "AkamaiGHost"
      },
      "body_patterns": [
        "Access Denied",
        "AkamaiGHost"
      ],
      "status_codes": [
        403
      ],
      "cookies": [
        "ak_bmsc"
      ]
    }
  },
  {
    "name": "ModSecurity",
    "vendor": "Trustwave SpiderLabs",
    "detection": {
      "headers": {
        "server": "Mod_Security",
        "x-mod-security": ".*"
      },
      "body_patterns": [
        "ModSecurity",
        "Mod_Security",
        "NOYB"
      ],
      "status_codes": [
        403,
        406
      ],
      "cookies": []
    }
  },
  {
    "name": "Imperva Incapsula",
    "vendor": "Imperva",
    "detection": {
      "headers": {
        "x-iinfo": ".*",
        "x-cdn": "Incapsula"
      },
      "body_patterns": [
        "incapsula",
        "_Incapsula_Resource",
        "Incapsula incident ID"
      ],
      "status_codes": [
        403
      ],
      "cookies": [
        "incap_ses",
        "visid_incap"
      ]
    }
  },
  {
    "name": "Sucuri",
    "vendor": "Sucuri Inc.",
    "detection": {
      "headers": {
        "server": "Sucuri/Cloudproxy",
        "x-sucuri-id": ".*"
      },
      "body_patterns": [
        "Sucuri WebSite Firewall",
        "Access Denied - Sucuri Website Firewall",
        "Questions?",
        "cloudproxy"
      ],
      "status_codes": [
        403
      ],
      "cookies": [
        "sucuri-"
      ]
    }
  },
  {
    "name": "Wordfence",
    "vendor": "Defiant Inc.",
    "detection": {
      "headers": {},
      "body_patterns": [
        "Generated by Wordfence",
        "wordfence",
        "Your access to this site has been limited"
      ],
      "status_codes": [
        403,
        503
      ],
      "cookies": [
        "wfvt_"
      ]
    }
  },
  {
    "name": "Barracuda",
    "vendor": "Barracuda Networks",
    "detection": {
      "headers": {
        "x-barracuda": ".*"
      },
      "body_patterns": [
        "barracuda",
        "You have been blocked"
      ],
      "status_codes": [
        403
      ],
      "cookies": []
    }
  },
  {
    "name": "F5 BIG-IP ASM",
    "vendor": "F5 Networks",
    "detection": {
      "headers": {
        "server": "BigIP",
        "x-cnection": ".*"
